<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0;net7.0;net6.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <SignAssembly>False</SignAssembly>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Yuh.Collections</Title>
    <Version>1.0.0-rc.1</Version>
    <AssemblyVersion>1.0.0-rc.1</AssemblyVersion>
    <FileVersion>1.0.0-rc.1</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Authors>YuHima</Authors>
    <Copyright>(C) 2024 YuHima</Copyright>
    <PackageProjectUrl>https://github.com/YuHima03/Yuh.Collections</PackageProjectUrl>
    <RepositoryUrl>https://github.com/YuHima03/Yuh.Collections</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>collection;deque;searching-algorithms</PackageTags>
    <Description>Provides collection types and searching methods.</Description>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageReleaseNotes>Fixed a critical bug in the `Deque&lt;T&gt;` class.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
